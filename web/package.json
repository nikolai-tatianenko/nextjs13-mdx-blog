{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "author": "Nikolai Tatianenko <nikolai.tatianenko@gmail.com>",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start:html": "npx http-server out",
    "start": "next start",
    "lint": "next lint",
    "lint-format": "next lint -- --fix && prettier --write .",
    "docs": "typedoc",
    "deploy": "npm run build && npx gh-pages -d out",
    "storybook": "concurrently \"npm:watch:*\"",
    "build-storybook": "concurrently \"npm:build:*\"",
    "build:css": "npx tailwindcss -i ./src/tailwind.css -o ./public/tailwind.css",
    "build:storybook": "storybook build",
    "build:storybook:docs": "storybook dev --docs",
    "watch:css": "npx tailwindcss -i ./src/tailwind.css -o ./public/tailwind.css --watch",
    "watch:storybook": "storybook dev -p 6006",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build"
  },
  "dependencies": {
    "@local/ds": "^0.0.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "20.3.1",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "classnames": "^2.3.2",
    "eslint-config-next": "13.4.6",
    "next": "13.4.6",
    "next-mdx-remote": "^4.4.1",
    "next-seo": "^6.0.0",
    "next-sitemap": "^4.1.3",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.22",
    "@storybook/addon-interactions": "^7.0.22",
    "@storybook/addon-links": "^7.0.22",
    "@storybook/addon-styling": "^1.3.0",
    "@storybook/blocks": "^7.0.22",
    "@storybook/nextjs": "^7.0.22",
    "@storybook/react": "^7.0.22",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.12",
    "gray-matter": "^4.0.3",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "storybook": "^7.0.22",
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "^4.0.0-next.14"
  }
}
